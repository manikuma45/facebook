<div class= "row">
 <div class="col-xs-10 col-xs-offset-1"> 
  <p id="notice"><%= notice %></p>
  <% @users.each do |user| %>
  <div class="col-lg-4 col-sm-6 col-xs-12">
    <ul class = "each_users">
      <li>
        <ul class= "data">
          <li><%= user.name %></li>
          <li><%= user.email %></li>
          <li><%= user.messages%></li>
        </ul>
      </li>
    </ul>
  </div>

  <% end %>
</div>   
</div>